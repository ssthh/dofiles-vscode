{
    "emmet.syntaxProfiles": {
      "javascript": "jsx"
    },
    "workbench.startupEditor": "newUntitledFile",
    // "editor.fontSize" : 14,
    "javascript.suggest.autoImports": true,
    "javascript.updateImportsOnFileMove.enabled": "always",
    "editor.rulers": [],
    "extensions.ignoreRecommendations": true,
    "typescript.tsserver.log": "off",
    "files.associations": {
      "*.tsx": "typescriptreact",
      ".env.*": "dotenv",
      ".prettierrc": "json"
    },
    "editor.parameterHints.enabled": false,
    "editor.renderLineHighlight": "gutter",
    //"editor.lineHeight" : 26,
    "material-icon-theme.languages.associations": {
      "dotenv": "tune"
    },
    "typescript.updateImportsOnFileMove.enabled": "never",
    "material-icon-theme.files.associations": {
      "tsconfig.json": "tune",
      "*.webpack.js": "webpack",
      "*.proto": "3d",
      "ormconfig.json": "database"
    },
    "material-icon-theme.activeIconPack": "nest",
    "editor.suggestSelection": "first",
    "explorer.confirmDelete": false,
    "gitlens.codeLens.recentChange.enabled": false,
    "terminal.integrated.showExitAlert": false,
  
    "[prisma]": {
      "editor.formatOnSave": true
    },
  
    "[javascript]": {
      "editor.formatOnSave": true
    },
    "[typescriptreact]": {
      "editor.formatOnSave": true
    },
    "typescript.suggest.autoImports": true,
    "terminal.integrated.env.osx": {
      "FIG_NEW_SESSION": "1",
      "CW_NEW_SESSION": "1"
    },
    // "editor.fontLigatures" : true,
    "emmet.includeLanguages": {
      "javascript": "javascriptreact"
    },
    "liveshare.featureSet": "insiders",
    "material-icon-theme.folders.associations": {
      "adapters": "contract",
      "grpc": "pipe",
      "kube": "kubernetes",
      "main": "lib",
      "websockets": "pipe",
      "implementations": "core",
      "protos": "pipe",
      "entities": "class",
      "kafka": "pipe",
      "use-cases": "functions",
      "migrations": "tools",
      "schemas": "class",
      "useCases": "functions",
      "eslint-config": "tools",
      "typeorm": "database",
      "_shared": "shared",
      "mappers": "meta",
      "fakes": "mock",
      "modules": "components",
      "subscribers": "messages",
      "domain": "class",
      "protocols": "contract",
      "infra": "app",
      "view-models": "views",
      "presentation": "template",
      "dtos": "typescript",
      "http": "container",
      "providers": "include",
      "factories": "class",
      "repositories": "mappings"
    },
    "cSpell.enableFiletypes": [
      "!asciidoc",
      "!c",
      "!cpp",
      "!csharp",
      "!go",
      "!handlebars",
      "!haskell",
      "!jade",
      "!java",
      "!latex",
      "!php",
      "!pug",
      "!python",
      "!restructuredtext",
      "!rust",
      "!scala",
      "!scss"
    ],
    "editor.acceptSuggestionOnCommitCharacter": false,
    "explorer.compactFolders": false,
    "git.enableSmartCommit": true,
    "editor.accessibilitySupport": "off",
    "explorer.confirmDragAndDrop": false,
    "terminal.integrated.fontSize": 14,
    "editor.semanticHighlighting.enabled": false,
    "editor.fontFamily": "JetBrainsMono Nerd Font",
    "editor.fontLigatures": true,
    "editor.letterSpacing": 0.4,
    "editor.lineHeight": 1.5,
    "editor.fontSize": 14,
    "editor.cursorWidth": 1,
    //"window.zoomLevel": 0.5,
    "editor.smoothScrolling": true,
    "editor.cursorBlinking": "smooth",
    "editor.cursorSmoothCaretAnimation": "on",
    "editor.minimap.enabled": false,
    "gitlens.codeLens.authors.enabled": false,
    "editor.tabSize": 2,
    "security.workspace.trust.untrustedFiles": "newWindow",
    "files.exclude": {
      "**/CVS": true,
      "**/.DS_Store": true,
      "**/.hg": true,
      "**/.svn": true,
      "**/.git": true
    },
    "gitlens.codeLens.enabled": false,
    "workbench.productIconTheme": "icons-carbon",
    "window.menuBarVisibility": "toggle",
    "editor.inlineSuggest.enabled": true,
    "github.copilot.enable": {
      "*": true,
      "plaintext": false,
      "markdown": false,
      "yaml": false,
      "typescript": true
    },
    "workbench.colorTheme": "Vesper",
    "workbench.iconTheme": "moxer-icons",
    "workbench.layoutControl.enabled": false,
    "editor.codeActionsOnSave": {
      "source.fixAll": "explicit",
      "source.fixAll.eslint": "explicit"
    },
    "diffEditor.wordWrap": "on",
    "go.toolsManagement.autoUpdate": true,
    "editor.defaultFormatter": "esbenp.prettier-vscode",
    "editor.formatOnSave": true,
    "workbench.settings.applyToAllProfiles": ["editor.formatOnSave"],

    "workbench.statusBar.visible": false,
    // Remove position indicator in the editor's scrollbar
    "editor.hideCursorInOverviewRuler": true,
    // Remove minimap
    // Move tabs to be in a single line with window controls
    "window.titleBarStyle": "native",
    "apc.electron": {
      "titleBarStyle": "hiddenInset",
      "trafficLightPosition": {
        "x": 11,
        "y": 10
      }
    },
    "apc.header": {
      "height": 36
    },
    // Remove unnecessary controls from primary bar and tabs list
    "apc.stylesheet": {
      ".title-label > h2": "display: none", // Remove primary side bar title
      ".title-actions": "display: none", // Remove primary side bar action icons
      ".editor-actions": "display: none", // Remove editor action icons
      ".nosidebar .inline-tabs-placeholder": "width: 75px" // Align tabs to not overlap window controls when primary bar is hidden
    },
    "apc.parts.font.family": {
      "sidebar": "Geist Mono",
      "titlebar": "Geist Mono",
      "activityBar": "Geist Mono"
    },
    "workbench.activityBar.location": "hidden",
    "git.autofetch": true
}